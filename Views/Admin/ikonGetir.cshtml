
@using KisiselWebProjesi.Models.Siniflar
@model KisiselWebProjesi.Models.ikonlar


@{
    ViewBag.Title = "ikonGetir";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>İkon Güncelle</h2>
<br />


@using (Html.BeginForm("ikonGuncelle", "Admin", FormMethod.Post))
{
    @Html.LabelFor(x => x.Id)
    @Html.TextBoxFor(x => x.Id, new { @class = "form-control", @readoly = "readonly" })
    <br />
    @Html.LabelFor(x=>x.ikon)
    @Html.TextBoxFor(x=>x.ikon, new {@class="form-control"})
    <br />
    @Html.LabelFor(x=>x.link)
    @Html.TextBoxFor(x=>x.link, new {@class="form-control"})
    <br />
    <button class="btn btn-success">İkon Güncelle</button>
}

